{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "RowezDPI Debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\build\\Debug\\rowezdpi.exe",
            "args": [
